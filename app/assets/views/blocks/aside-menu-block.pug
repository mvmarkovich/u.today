include top-bots.pug

include top-miners.pug

.accordion.accordion--open
    .accordion__title(@click="accordionOpen") Subcategories
    .accordion__body
        ul.tabs.tabs--vertical
            each item, i in nav[0].childs
                li.tabs__item(class=[ i === 1 ? 'tabs__item--active' : '' ])
                    a.tabs__btn.btn.btn--pseudo(href="#!")= item.name